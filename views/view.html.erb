<% name = 'Nevily' %>
<% dishes = ['pasta', 'cesar salad', 'risotto'] %>
<% menu_items = ['home', 'about us', 'products', 'services', 'contact']%>

<header> 
	<h1> Logo and title go here </h1>
	<ul>
		<% menu_items.each do |item| %>
			<li><a href="#"> <%= item.capitalize %></a></li>
		<% end %>
	</ul>
</header>

<div> 
	<h2>Title goes here.</h2>
	<p> Hi I am <%= name %>, i would like you to check out my website and let me know what you think! </p>
	<p> I love cooking, especially italian food.</p>
	My top 3 dishes are:
	<ul>
		<% dishes.each do |dish| %>
			<li><%= dish.capitalize %></li>
		<% end %>
	</ul>
</div>

<footer>
	<h5> Website made by <%= name %> </h5>
</footer>